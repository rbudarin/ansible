server {
    listen       {{ lighthouse_port }};
    listen       [::]:{{ lighthouse_port }};
    server_name  localhost;
    root         {{ lighthouse_path }};
    include /etc/nginx/default.d/*.conf;
    error_page 404 /404.html;
    location = /404.html {
    }
    error_page 500 502 503 504 /50x.html;
    location = /50x.html {
    }
}